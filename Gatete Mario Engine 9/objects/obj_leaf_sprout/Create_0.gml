/// @description A leaf that came out from a block

//Inherit the parent event
event_inherited();

//Ignore platforms
ignore_platforms = true;

//Do not swim
noswim = true;

//Do not animate
image_speed = 0;
image_index = 0;

//Action
action = "sprout";

//Set the vertical speed
yspeed = -2.5;
yadd = 0.0825;

//Sequence
sequence = 4;

//Give points when picked up?
noscore = false;